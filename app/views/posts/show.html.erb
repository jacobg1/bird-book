<%= link_to 'take a guess', new_post_guess_path(@post) %>
<img src="<%= @post.photo_url %>"/>
<% @post.guesses.each do |guess| %>
  <p><%= guess.guess_text %>, <%= link_to 'edit', edit_post_guess_path(@post, guess) %> <%= link_to 'delete', post_guess_path(@post, guess), method: :delete %>, <%= link_to '(+)', post_guess_path(@post, guess)  %> <%= guess.score %> </p>
<% end %>
