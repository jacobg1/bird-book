<%= link_to "&larr; Back".html_safe, :back, :class => 'dropbtn' %>
<div class="tag-show-wrapper">
<div class="tag">

<h3 class='tag-show-content'><%= @category.category_text %></h3>

</div>

<% @posts.each do |post| %>
<h1 class='sub-title'><%= post.title %></h1>

<object data="<%= post.photo_url %>" type="image/png" class="photo">
<img src="https://www.justpro.co/img/no-image.png" class="default-photo"/>
</object>

<br>
<% end %>
</div>
